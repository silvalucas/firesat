<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CadSquadController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" layoutY="6.0" prefHeight="75.0" prefWidth="600.0" text="CADASTRAR ESQUADRÃO">
               <font>
                  <Font name="Segoe UI" size="24.0" />
               </font>
            </Label>
            <VBox layoutY="95.0" prefHeight="242.0" prefWidth="600.0">
               <children>
                  <Label alignment="CENTER" prefHeight="39.0" prefWidth="600.0" text="NOME">
                     <font>
                        <Font name="Segoe UI" size="12.0" />
                     </font>
                  </Label>
                  <TextField prefHeight="31.0" prefWidth="420.0" promptText="3º BATALHÃO INFANTARIA ">
                     <VBox.margin>
                        <Insets left="90.0" right="90.0" />
                     </VBox.margin>
                  </TextField>
                  <Label alignment="CENTER" prefHeight="39.0" prefWidth="600.0" text="ESPECIALIDADE">
                     <font>
                        <Font name="Segoe UI" size="12.0" />
                     </font>
                  </Label>
                  <TextField prefHeight="31.0" prefWidth="420.0" promptText="INCENDIO LARGA ESCALA">
                     <VBox.margin>
                        <Insets left="90.0" right="90.0" />
                     </VBox.margin>
                  </TextField>
                  <Label alignment="CENTER" prefHeight="39.0" prefWidth="600.0" text="QUANTIDADE SOLDADOS">
                     <font>
                        <Font name="Segoe UI" size="12.0" />
                     </font>
                  </Label>
                  <TextField prefHeight="31.0" prefWidth="420.0" promptText="12,20,32...">
                     <VBox.margin>
                        <Insets left="90.0" right="90.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
            <Button layoutX="14.0" layoutY="30.0" mnemonicParsing="false" onAction="#goToSquad" prefHeight="25.0" prefWidth="33.0" style="-fx-background-color: none;">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/back.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button layoutX="224.0" layoutY="330.0" mnemonicParsing="false" onAction="#concludeCadSquad" prefHeight="39.0" prefWidth="152.0" styleClass="botoes" text="CONCLUIR" />
         </children>
      </Pane>
   </children>
</AnchorPane>
